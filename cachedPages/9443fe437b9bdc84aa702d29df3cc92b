                        <h4 class="{{title_class}}">{{{title}}}
                    
                    <div class="modal-body {{body_class}}">
                        {{{body}}}
                    
                    <div class="modal-footer {{footer_class}}">
                    
                
            
        
    </script><script>
        (function () {
            $('.navbar-nav').find('.active').removeClass('active');
            $('.navbar-nav').find('a[href="/onsite-auctions"]').addClass('active');
        })();
        initPage = function () {
            
        };
        function dtWriter(rowIndex, record, columns, cellWriter) {
            var div = '<div class="panel panel-primary"><div class="panel-heading"><h4 class="panel-title">' + record.Heading + '<div id="collapse_' + rowIndex + '" class="panel"><div class="panel-body">' + record.Body + '';
            return div;
        }
        function dtReader(index, div, record) {
            var $div = $(div)
            record.Heading = $div.find('> .panel-heading .panel-title').text()
            record.Body = $div.find('> .panel-body').html();
        }
        function initDynaTable() {
            $('#onsite-items-container').on('dynatable:afterUpdate', function () {
                if (!$('#dynatable-search-onsite-items-container').hasClass('col-lg-12')) {
                    $('#dynatable-search-onsite-items-container, .dynatable-per-page').addClass('col-lg-12').wrapAll('<div class="row text-left" />');
                    $('#dynatable-per-page-onsite-items-container, #dynatable-query-search-onsite-items-container').addClass('form-control');
                }
                if ($('#custom-pager').length == 0) {
                    $('#dynatable-pagination-links-onsite-items-container').clone().attr('id', 'custom-pager').css({ 'margin-top': '20px' }).wrap('<div class="col-lg-12" />').parent().insertAfter('#dynatable-search-onsite-items-container');
                }
                else {
                    $('#custom-pager').html($('#dynatable-pagination-links-onsite-items-container').html());
                    $('html, body').animate({ scrollTop: $('#onsite-items-panel').offset().top }, 800);
                }
            });
            var dyna = $('#onsite-items-container').dynatable({
                features: {
                    paginate: true,
                    sort: false,
                    pushState: false,
                    search: true,
                    recordCount: true,
                    perPageSelect: false
                },
                table: {
                    bodyRowSelector: '> .panel'
                },
                writers: {
                    _rowWriter: dtWriter
                },
                readers: {
                    _rowReader: dtReader
                },
                dataset: {
                    perPageDefault: 25
                }
            }).data('dynatable');
            var selector = '#custom-pager a.' + dyna.settings.inputs.paginationLinkClass + ':not(.' + dyna.settings.inputs.paginationActiveClass + ',.' + dyna.settings.inputs.paginationDisabledClass + ')';
            $(document).undelegate(selector, 'click.dynatable');
            $(document).delegate(selector, 'click.dynatable', function (e) {
                var $this = $(this);
                dyna.paginationPage.set($this.data('dynatable-page'));
                dyna.process();
                if (e.preventDefault)
                    e.preventDefault();
            });
        }
    </script><script src="/Scripts/NewFeatures.min.js" async="async"></script></body></html>


