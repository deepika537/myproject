                            if (typeof ($.connection) == 'undefined') { return; }
                            var signalR = $.connection.bidNotificationHub;
                            if (typeof (signalR) == 'undefined' || typeof (signalR.client) == 'undefined') { return; }
                            signalR.client.notifyOutBid = notifyOutBid;
                            signalR.client.notifyBidPlaced = notifyBidPlaced;
                            $.connection.hub.start().done(function () {
                                signalR.server.monitorAuctionBids(loggedInUser);
                            });
                        });
                    });
                });
                function notifyOutBid(bid, auction) {
                    if (loggedInUser && loggedInUser.id != bid.user_ID) {
                        var msg = '';
                        msg += '<strong>' + (bid.user_nickname ? bid.user_nickname : 'Somebody') + '';
                        msg += ' placed a higher bid than you ';
                        msg += '(' + NiceCurrency('R', bid.bid_amount) + ')';
                        jBoxNotice({ title: ('<a style="color:#FFF;" href="' + auction.auction_link + '">' + (auction.auction_title ? auction.auction_title : 'Bid Notification') + ''), content: msg, color: 'blue', stack: true, delayClose: 9000, audio: '/files/audio/bling1' });
                    }
                }
                function notifyBidPlaced(bid, auction) {
                    $(document).trigger('bid-notification-showed', { 'bid': bid, 'auction': auction });
                }
            })();
        } catch (e) {
            console.error(e);
        }
    </script><script id="modal-template" type="text/x-handlebars-template">
