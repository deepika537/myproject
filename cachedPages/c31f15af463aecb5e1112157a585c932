$(function() {
    var currentDate = new Date();
    var currentHour = currentDate.getHours();
    switch(true) {
        case (currentHour >= 0 && currentHour < 6):
            $('#kid_saluting_banner').show();
            break;
        case (currentHour >= 6 && currentHour < 12):
            assignImages('summer');
            $('#seasonal_banner').show();
            break;
        case (currentHour >= 12 && currentHour < 18):
            $('#kid_saluting_banner').show();
            break;
        case (currentHour >= 18):
            assignImages('summer_rain');
            $('#seasonal_banner').show();
            break;
        default:
            assignImages('summer');
            $('#seasonal_banner').show();
    }
});

function assignImages(season){
    var data = $.grep(seasonImagesData, function (e) {
        return e.season === season
    });

    $('#header').css('background','url("' +    data[0].srcGovernmentLiquidation + '") no-repeat left top');
    $('#imgGovernmentLiquidation').attr('src', data[0].srcGovernmentLiquidation);
    $('#imgMilitaryTruck').attr('src',         data[0].srcMilitaryTruck        );
    $('#imgSurplusBoots').attr('src',          data[0].srcSurplusBoots         );
    $('#imgAircraftParts').attr('src',         data[0].srcAircraftParts        );
    $('#imgAmmoCans').attr('src',              data[0].srcAmmoCans             );
    $('#imgTrailer').attr('src',               data[0].srcTrailer              );
    $('#imgSurplusTires').attr('src',          data[0].srcSurplusTires         );
}

function showGray(fileName) {
    $('#overheader').stop(true, true);
    $('#overheader').css('opacity', '0');
    $('#overheader').css('background-image', "url('" + fileName + "')").animate({ opacity: 1.0 }, 500);
}

function hideGray() {
    $('#overheader').animate({ opacity: 0 }, 800, "linear");
}
