<script>
// When the DOM is ready open the modal after 5 seconds
$(function() {
    setTimeout(openModal,5000);

    // When the user clicks the X in the modal, close it
    $("#theParachuteModal .close").click(function() {
        $(this).parent().parent().css('display','none');
    });

    // When the escape key is pressed close the modal
    $(document).keyup(function(e) {
        if (e.keyCode == 27) { // escape key maps to keycode `27`
            $('.modal').css('display','none');
        }
    });

    $("form").submit(function(event) {
        setCookie('theParachuteOptedIn','Yes','90');
        return true;
    });
});

function openModal() {
    //Show the Parachute Opt-in popup modal only once per session (30 days)
    if ( getCookie('theParachuteDisplayed') == null &&
         getCookie('theParachuteOptedIn') == null ) {
        $('#theParachuteModal').css('display','block');
        setCookie('theParachuteDisplayed','Yes','30');
    }
};
