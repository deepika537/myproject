                }
                return xhr;
            }

            var xhr = null,
                param;

            xhr = _getXMLHttpRequest();
            if( xhr ){
                if( params ){
                    url += "?";
                    for( param in params ){
                        url += param + "=" + params[param] + "&";
                    }
                    url = url.substr( 0, url.length - 1 );
                }else{
                    params = {};
